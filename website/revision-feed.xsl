<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:atom="http://www.w3.org/2005/Atom">	<xsl:template match="atom:feed">		<xsl:for-each select="atom:entry">			<a style="font-size: 9pt; font-family: arial,sans-serif; height: 12pt; line-height: 12pt">				<xsl:attribute name="href">					<xsl:apply-templates select="atom:link"/>				</xsl:attribute>				<xsl:attribute name="title">by <xsl:value-of select="atom:author/atom:name"/> at <xsl:value-of select="atom:updated"/>				</xsl:attribute>				<xsl:value-of select="atom:title"/>			</a>			<br/>		</xsl:for-each>	</xsl:template>	<xsl:template match="atom:link">		<xsl:value-of select="@href"/>	</xsl:template></xsl:stylesheet>